{
    "name": "cloudmanic/craft-nginx",
    "description": "Manage nginx from craft.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "nginx"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cloudmanic Labs, LLC",
            "homepage": "https://cloudmanic.com"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.19"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "cloudmanic\\nginx\\": "src/"
        }
    },
    "extra": {
        "name": "Nginx",
        "handle": "craft-nginx",
        "schemaVersion": "1.0.0",
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/cloudmanic/craft-nginx/master/CHANGELOG.md",
        "class": "cloudmanic\\nginx\\Nginx"
    }
}
