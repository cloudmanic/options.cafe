{
    "name": "cloudmanic/craft-sitemap",
    "description": "An easy way to add a sitemap to the CMS.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "sitemap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cloudmanic Labs, LLC",
            "homepage": "https://cloudmanic.com"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.19"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "cloudmanic\\sitemap\\": "src/"
        }
    },
    "extra": {
        "name": "Sitemap",
        "handle": "craft-sitemap",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/cloudmanic/craft-sitemap/master/CHANGELOG.md",
        "class": "cloudmanic\\sitemap\\Sitemap"
    }
}
