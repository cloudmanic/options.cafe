{
  "require": {
    "craftcms/cms": "3.0.41.1",
    "vlucas/phpdotenv": "^2.4.0",
    "mmikkel/cp-field-inspect": "1.0.5",
    "craftcms/redactor": "2.1.6",
    "dolphiq/sitemap": "v1.0.9",
    "craftcms/aws-s3": "1.0.8",
    "itscoding/facebook-connector": "v1.3.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "OptionsCafe\\": "modules/options-cafe/",
      "CloudmanicCraftNginx\\": "modules/cloudmanic-craft-nginx/",
      "CloudmanicCraftSendy\\": "modules/cloudmanic-craft-sendy/",
      "CloudmanicCraftBrokerFinder\\": "modules/cloudmanic-craft-broker-finder/"     
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
